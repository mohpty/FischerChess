<section>
    <div class="container-fluid" style="background-color:whitesmoke">
        <div class="container">
            <div class="row my-4" style='padding-top:25px;'>
                <div class="col-8">
                    <div class="container py-2">
                        <div class="chessBoard">
                            <div id=''>
                                <div id="promotion" class="row">
                                    <div class="col-3 mx-auto">
                                        <div class="mx-auto text-center">
                                            <img id="promotion-bB" class='promotion_piece blackPromPiece' src="data:image/svg+xml;base64," alt="black bishop">
                                            <img id="promotion-wB" class='promotion_piece whitePromPiece' src="data:image/svg+xml;base64," alt="white bishop">
                                        </div>
                                    </div>
                                    <div class="col-3 mx-auto">
                                        <div class="mx-auto text-center">
                                            <img id='promotion-bR' class='promotion_piece blackPromPiece' src="data:image/svg+xml;base64," alt="black rook">
                                            <img id='promotion-wR' class='promotion_piece whitePromPiece' src="data:image/svg+xml;base64," alt="white rook">
                                        </div>
                                    </div>
                                    <div class="col-3 mx-auto">
                                        <div class="mx-auto text-center">
                                            <img id='promotion-bN' class='promotion_piece blackPromPiece' src="data:image/svg+xml;base64," alt="black knight">
                                            <img id='promotion-wN' class='promotion_piece whitePromPiece' src="data:image/svg+xml;base64," alt="white knight">
                                        </div>
                                    </div>
                                    <div class="col-3 mx-auto">
                                        <div class="mx-auto text-center">
                                            <img id='promotion-bQ' class='promotion_piece blackPromPiece' src="data:image/svg+xml;base64," alt="">
                                            <img id='promotion-wQ' class='promotion_piece whitePromPiece' src="data:image/svg+xml;base64," alt="">
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div id="myBoard" style="width: 600px; margin:auto"></div>
                        </div>
                    </div>
                </div>
                <div class="col-4 border">
                    <div class="container">
                        <div class="matchMakingButtons mx-auto text-center py-4">
                            <button id='createGame' class='btn btn-primary'>Create game</button>
                            <br>
                            <input id='gameId' type="number" placeholder="Enter Room Id">
                            <button id='joinGame' class='btn btn-success my-2'>Join game</button>
                        </div>
                        <div id='matchMakingStatus' class="container mx-auto text-center py-4">
                        </div>
                        
                    </div>
                    <div class="container">
                        <div id='resignationButton' class="btn btn-danger my-2">Resign</div>
                        <p id="gameStatus" style='display:none;'></p>
                        <!-- <h4>Game PGN:</h4> -->
                        <div id="gamePGN" class='py-2' style="font-family: monospace"></div>
                    </div>
                </div>
    
    
            </div>
        </div>
    </div>
</section>


<script>const USER = "<%=session.user.id%>"</script>
<script type='module' src="/main.js"></script>